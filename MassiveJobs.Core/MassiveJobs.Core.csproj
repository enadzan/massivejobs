<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.1.3</Version>
    <RepositoryUrl>https://github.com/enadzan/massivejobs</RepositoryUrl>
    <PackageLicenseExpression>LGPL-3.0-only</PackageLicenseExpression>
    <Authors>MassiveJobs.NET</Authors>
    <Copyright></Copyright>
    <Description>Core functionality for scheduled and "out-of-band" jobs publishing and running in .NET. This package only contains in-memory implementation of the message broker which only works withing one process. For distributed job publishing use MassiveJobs.RabbitMqBroker or similar package.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.4" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="3.1.4" />
    <PackageReference Include="System.Text.Json" Version="4.7.2" />
  </ItemGroup>

</Project>
